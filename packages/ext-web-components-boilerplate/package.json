{
  "name": "@sencha/ext-web-components-boilerplate",
  "version": "7.1.0",
  "description": "ext-web-components-boilerplate",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "build": "npm run clean && npm run webpackprod && npm run gzip && npm run http",
    "clean": "rimraf build",
    "webpackprod": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/",
    "gzip": "npm run gzipbuild && npm run gzipext && npm run gzipresources",
    "gzipbuild": "npx gzip build/*.js",
    "gzipext": "npx gzip build/ext/*.js",
    "gzipresources": "npx gzip build/resources/*.js",
    "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
    "http": "npx http-server build -g -o"
  },
  "devDependencies": {
    "@sencha/ext-web-components": "~7.0.0",
    "@sencha/ext-webpack-plugin": "~7.0.0",

    "@sencha/ext": "~7.0.0",
    "@sencha/ext-modern": "~7.0.0",
    "@sencha/ext-modern-treegrid": "~7.0.0",
    "@sencha/ext-modern-theme-material": "~7.0.0",

    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",

    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",

    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",

    "@webcomponents/webcomponentsjs": "^2.2.10",
    "rimraf": "^2.7.0",
    "cross-env": "^5.2.0",
    "fs-extra": "^8.1.0",
    "portfinder": "^1.0.21",
    "gzip-cli": "^1.0.1",
    "replace-in-file": "^4.1.2",
    "http-server": "^0.11.1",

    "html-webpack-plugin": "^3.2.0",
    "base-href-webpack-plugin": "~2.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "webpack-visualizer-plugin": "^0.1.11",
    "terser-webpack-plugin": "^1.4.1",
    "compression-webpack-plugin": "^3.0.0",

    "eslint-loader": "^2.2.1",
    "eslint": "^6.1.0",
    "babel-loader": "^8.0.6",
    "babel-eslint": "^10.0.2",
    "html-loader": "^0.5.5",
    "style-loader": "^1.0.0",
    "css-loader": "^3.2.0",
    "sass-loader": "^7.2.0",
    "node-sass": "^4.12.0",

    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {}
}
