{
  "name": "ext-components",
  "version": "7.0.0",
  "description": "Use Ext JS with Web Components",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx lerna bootstrap --loglevel warn",
    "increment": "npx lerna publish --skip-git --skip-npm --force-publish",
    "clean": "npx lerna clean",
    "install:clean": "npm cache clear --force && npx lerna clean && npm install",
    "kitchensink": "cd packages/ext-components-kitchensink && npm start",
    "boilerplate": "cd packages/ext-components-boilerplate && npm start",
    "serve-tests": "cd packages/ext-components-tests && npm start"
  },
  "keywords": [
    "Ext",
    "JS",
    "Sencha",
    "Web Components"
  ],
  "author": "Sencha, Inc.",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^3.13.0"
  }
}
